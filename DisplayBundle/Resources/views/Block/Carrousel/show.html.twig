<div class='contentCaroussel'> 
    <div id='{{ carrousel_id }}'>
        <div u='loading'>
            <div></div>
            <div></div>
        </div>
        <div u='slides' style='cursor: move; position: absolute; left: 0px; top: 0px; width: {{ width }}; height: {{ height }}; overflow: hidden;'>
              {% for myPictures in pictures %}
                <div><img alt='image' src='{{ myPictures.src }}' /></div>
            {% endfor %}
        <span u='arrowleft' class='jssora12l' style='width: 30px; height: 46px; top: 123px; left: 0px;'></span>
        <span u='arrowright' class='jssora12r' style='width: 30px; height: 46px; top: 123px; right: 0px'></span>
        <a style='display: none' href='http://www.jssor.com'>Carrousel</a>
        </div>
    </div>
</div>

<script type="text/javascript">
    function callCss(file){
        var linkElement = document.createElement("link");
        linkElement.rel = "stylesheet";
        linkElement.href = file;
        document.head.appendChild(linkElement);
    }

    function callJs(file){
        var scriptElement = document.createElement("script");
        scriptElement.type = "text/javascript";
        scriptElement.src = file;
        document.head.appendChild(scriptElement);
    }

    callCss('/bundles/phporchestradisplay/block/Carrousel/css/style.css');

    callJs('/bundles/phporchestradisplay/block/Carrousel/js/script.js');
    callJs('/bundles/phporchestradisplay/jssor/js/jssor.core.js');
    callJs('/bundles/phporchestradisplay/jssor/js/jssor.slider.js');
    callJs('/bundles/phporchestradisplay/jssor/js/jssor.utils.js');
</script>
