<div id="{{galleryId}}" class="gallery {{ galleryClass }}">
    <div>
    {% for picture in pictures %}<a 
        class="fancybox-thumb"
        rel="fancybox-thumb"
        href="{{ get_media_format_url(picture, imageFormat) }}"
        title="{{ 'php_orchestra_display.block.gallery.no_title_defined'|trans }}"
       ><img src="{{ get_media_format_url(picture, thumbnailFormat) }}" alt="" class="gallery-picture" /></a>{% endfor %}
    </div>
</div>
{% if (numberOfPages > 1) %}
    <div class="gallery-navigator">
        {{ navigator(numberOfPages, currentPage, parameters) }}
    </div>
{%endif%}

<script src="/bundles/phporchestradisplay/js/requireCss.js"></script>

<script type="text/javascript">
    if (typeof orchestraGalCol == 'undefined') {
        var orchestraGalCol = {};
    }
    orchestraGalCol['{{galleryId}}'] = {{ numberOfColumns }};

    loadCss('/bundles/phporchestradisplay/fancybox/source/jquery.fancybox.css?v=2.1.5');
    loadCss('/bundles/phporchestradisplay/fancybox/source/helpers/jquery.fancybox-buttons.css?v=1.0.5');
    loadCss('/bundles/phporchestradisplay/fancybox/source/helpers/jquery.fancybox-thumbs.css?v=1.0.7');
    loadCss('/bundles/phporchestradisplay/block/Gallery/css/style.css');
</script>

<script data-main="/bundles/phporchestradisplay/block/Gallery/js/script.js" src="/bundles/phporchestradisplay/js/require.js"></script>
