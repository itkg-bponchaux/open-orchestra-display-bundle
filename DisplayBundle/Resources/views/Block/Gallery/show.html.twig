{{ navigator('http://demo-phporchestra.dev/app_dev.php/page-start-orchestra', nbPages, curPage, params) }}

<div id="{{galleryId}}" class="gallery {{ galleryClass }}">
    <div>
    {% for picture in pictures %}<a class="fancybox-thumb" rel="fancybox-thumb" href="{{ get_media_format_url(picture, imageFormat) }}" title="Title not defined"><img src="{{ get_media_format_url(picture, thumbnailFormat) }}" alt="" class="gallery-picture" /></a>{% endfor %}
    </div>
</div>

<script type="text/javascript">
    if (typeof orchestraGalCol == 'undefined') {
        var orchestraGalCol = {};
    }
    orchestraGalCol['{{galleryId}}'] = {{ nbColumns }};

    function callCss(file){
        var linkElement = document.createElement("link");
        linkElement.rel = "stylesheet";
        linkElement.href = file;
        document.head.appendChild(linkElement);
    }

    function callJs(file){
        var scriptElement = document.createElement("script");
        scriptElement.type = "text/javascript";
        scriptElement.src = file;
        document.head.appendChild(scriptElement);
    }

    callCss('/bundles/phporchestradisplay/fancybox/source/jquery.fancybox.css?v=2.1.5');
    callCss('/bundles/phporchestradisplay/fancybox/source/helpers/jquery.fancybox-buttons.css?v=1.0.5');
    callCss('/bundles/phporchestradisplay/fancybox/source/helpers/jquery.fancybox-thumbs.css?v=1.0.7');
    callCss('/bundles/phporchestradisplay/block/Gallery/css/style.css');

    callJs('/bundles/phporchestradisplay/fancybox/source/jquery.fancybox.pack.js?v=2.1.5');
    callJs('/bundles/phporchestradisplay/fancybox/source/helpers/jquery.fancybox-buttons.js?v=1.0.5');
    callJs('/bundles/phporchestradisplay/fancybox/source/helpers/jquery.fancybox-media.js?v=1.0.6');
    callJs('/bundles/phporchestradisplay/fancybox/source/helpers/jquery.fancybox-thumbs.js?v=1.0.7');

    callJs('/bundles/phporchestradisplay/block/Gallery/js/script.js');
</script>